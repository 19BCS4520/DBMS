create table emp(emp_name varchar(20),city varchar(20));
insert into emp values('parikshit','H.P');
insert into emp values('sarab','punjab');
insert into emp values('akhil','Harayana');
insert into emp values('prabhav','J.K');
select distinct *from emp;
create table emp_sal(emp_name varchar(20),dep varchar(20),salary number(10));
insert into emp_sal values('parikshit','CSE',900000);
insert into emp_sal values('sarab','it',70000);
insert into emp_sal values('akhil','civil',50000);
insert into  emp_sal values('prabhav','music',35000);
select distinct *from emp_sal;
select distinct emp.emp_name,emp.city,emp_sal.emp_name,emp_sal.dep,emp_sal.salary from emp right join emp_sal on emp.emp_name=emp_sal.emp_name;
select distinct emp.emp_name,emp.city,emp_sal.emp_name,emp_sal.dep,emp_sal.salary from emp left join emp_sal on emp.emp_name=emp_sal.emp_name;
